import java.util.*;
class prog
{
public static void main(String[] args)
{
Scanner sc=new Scanner(System.in);
String str=sc.nextLine();
int n=str.length();
String w[]=str.split(" ");
for(String word:w)
{
    for(int i=word.length()-1;i>=0;i--)
    {
        System.out.print(word.charAt(i));
    }
    System.out.print(" ");
}
}
}
